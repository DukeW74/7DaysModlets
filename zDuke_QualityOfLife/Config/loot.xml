<configs>
<!-- empty, storage containers the player crafts-->
<set xpath="/lootcontainers/lootcontainer[@id='1']/@size">10,9</set>
<set xpath="/lootcontainers/lootcontainer[@id='34']/item[@group='cannedfood']/@prob">0.5</set>

<append xpath="/lootcontainers/lootgroup[@name='dyes']">
	<item name="modDyeYellow"/>
	<item name="modDyePurple"/>
	<item name="modDyeOrange"/>
</append>

<append xpath="/lootcontainers" >	

<!-- Cooler Sound -->
<lootcontainer id="209" count="0" size="10,9" sound_open="UseActions/open_cooler" sound_close="UseActions/close_cooler" loot_quality_template="baseTemplate">
	<item name="cobweb" count="1,2"/>
</lootcontainer>

<!-- Medicine Cabinet Sound -->
<lootcontainer id="210" count="0" size="10,9" sound_open="UseActions/open_medicine_cabinet" sound_close="UseActions/close_medicine_cabinet" loot_quality_template="baseTemplate">
