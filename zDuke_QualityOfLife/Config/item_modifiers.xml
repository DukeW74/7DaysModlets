<configs>

	<!-- Buffs mining related mods -->
	<set xpath='/item_modifiers/item_modifier[@name="modMeleeBunkerBuster"]/effect_group/passive_effect[@name="DamageModifier" and @value=".15"]/@value'>.25</set>
